   "source": [
    "from traitlets import HasTraits, List, All\n",
    "class Scene(HasTraits):\n",
    "    targets = List(eventful=True)\n",
    "\n",
    "scene = Scene()\n",
    "\n",
    "def on_change(change):\n",
    "    print(\"change:\", change)\n",
    "scene.observe(on_change, type=All)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 19,
   "metadata": {},
   "outputs": [],
   "source": [
    "scene.targets.extend([1,2,3])"
   ]
